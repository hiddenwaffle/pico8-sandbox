pico-8 cartridge // http://www.pico-8.com
version 18
__lua__

function _init()
  x = 0
  y = 8
  xspeed = 1
  yspeed = 1
end

function _update60()
  x += xspeed
  y += yspeed
  if x > 127 or x < 0 then
    xspeed *= -1
  end
  if y > 127 - 8 or y < 0 + 8 then
    yspeed *= -1
  end
end

function _draw()
  cls(1)
  rect(0, 0 + 8, 127, 127 - 8, 7)
  circfill(x, y, 4, 9)
end

