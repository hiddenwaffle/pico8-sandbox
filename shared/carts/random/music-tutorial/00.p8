g = { }

function _init()
  g.x = 64
  g.y = 64
  g.dx = 1
  g.dy = 1.5
end

function _update60()
  g.x += g.dx
  g.y += g.dy
  if g.x < 0 or g.x + 16 > 127 then
    sfx(5)
    g.dx *= -1
  end
  if g.y < 0 or g.y + 16 > 127 then
    sfx(5)
    g.dy *= -1
  end
end

function _draw()
  -- cls(1)
  for i = 1, 15 do
    line(0, flr(rnd(128)), 128, flr(rnd(128)), 1)
  end
  spr(1, g.x, g.y, 2, 2)
end

